@model DataLayer.Models.Erfarenhet

@{
    ViewData["Title"] = "Redigera Erfarenhet";
}

<h2>Redigera Erfarenhet</h2>

<form asp-action="EditErfarenhet" asp-controller="account" method="post">
    <input type="hidden" asp-for="Id" />
    <div class="form-group">
        <label asp-for="Title">Titel</label>
        <input type="text" asp-for="Title" class="form-control">
    </div>
    <div class="form-group">
        <label asp-for="Beskrivning">Beskrivning</label>
        <textarea asp-for="Beskrivning" class="form-control"></textarea>
    </div>
    <button type="submit" class="btn btn-primary">Uppdatera</button>
</form>




