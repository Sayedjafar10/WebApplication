@model Utbildning

@{
    ViewData["Title"] = "Redigera Utbildning";
}

<h2>Redigera Utbildning</h2>

<form asp-action="EditUtbildning" asp-controller="account" method="post">
    <input type="hidden" asp-for="Id" />
    <div class="form-group">
        <label asp-for="Namn">Namn</label>
        <input type="text" asp-for="Namn" class="form-control">
    </div>
    <div class="form-group">
        <label asp-for="Beskrivning">Beskrivning</label>
        <textarea asp-for="Beskrivning" class="form-control"></textarea>
    </div>
    <button type="submit" class="btn btn-primary">Uppdatera</button>
</form>
